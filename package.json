    "serve": "gatsby build && NODE_ENV=production gatsby serve",
    "build": "gatsby build",
    "dev": "gatsby develop",
    "lint": "eslint . --ext .js --cache",
    "test": "start-test dev 8000 cy:run",
    "test:open": "start-test dev 8000 cy:open",
    "cy:run": "env-cmd cypress run --record",
