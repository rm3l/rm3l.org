    "cy:open": "cypress open",
    "ci:cy:run": "env-cmd cypress run --reporter cypress-multi-reporters --reporter-options configFile=cypress-recorder-config.json --record",
    "ci:cy:run:no-record": "env-cmd cypress run --reporter cypress-multi-reporters --reporter-options configFile=cypress-recorder-config.json",
    "ci:test": "start-test dev 8000 ci:cy:run",
    "ci:test:no-record": "start-test dev 8000 ci:cy:run:no-record",
    "snyk-protect": "snyk protect"
  },
